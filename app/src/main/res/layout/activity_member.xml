<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.MemberActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="0dp">

        <Button
            android:id="@+id/btn_M_AddOrder"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#A83D63"
            android:text="ADD ORDER" />

        <Button
            android:id="@+id/btn_M_ViewAllCupcake"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#A73D63"
            android:text="VIEW ALL CUPCAKE" />

        <Button
            android:id="@+id/btn_M_Back"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#A63D63"
            android:text="BACK" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="412dp"
            android:layout_height="726dp"
            android:src="@drawable/member" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>